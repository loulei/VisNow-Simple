<?xml version="1.0" encoding="UTF-8"?>

<module name="read VASP" class="ReadVASP">
    <description value="Reads VASP density data."/>
    <outputs>
        <output name="volume" type="pl.edu.icm.visnow.lib.types.VNRegularField">
            <schema>
                <param name="NSPACE" value="3"/>
                <param name="NDIMS" value="3"/>
                <param name="AFFINE" value="true"/>
            </schema>
        </output>
        <output name="atoms" type="pl.edu.icm.visnow.lib.types.VNIrregularField">
            <schema>
                <param name="IRREGULAR" value="true"/>
                <param name="NSPACE" value="3"/>
            </schema>
        </output>
        <geometryOutput/>
    </outputs>
</module>

