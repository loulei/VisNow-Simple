<?xml version="1.0" encoding="UTF-8"?>
<module name="accumulate timesteps" class="AccumulateTimesteps">
   <inputs>
       <input name="inField" type="pl.edu.icm.visnow.lib.types.VNField" modifiers="NECESSARY:TRIGGERING">
       </input>
    </inputs>
    <outputs>
        <output name="outField" type="pl.edu.icm.visnow.lib.types.VNField">
            <schema>
                <param name="IRREGULAR" value="true"/>
            </schema>
        </output>
        <output name="regularOutField" type="pl.edu.icm.visnow.lib.types.VNRegularField">
            <schema>
                <param name="REGULAR" value="true"/>
            </schema>
        </output>
        <geometryOutput/>
    </outputs>
    <description value="takes a series of input fields with constant structures and merges to a time-dependent field"/>
</module>
