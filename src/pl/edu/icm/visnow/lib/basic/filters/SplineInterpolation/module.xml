<?xml version="1.0" encoding="UTF-8"?>

<module name="spline interpolation" class="SplineInterpolation">

    <inputs>
       <input name="inField" type="pl.edu.icm.visnow.lib.types.VNField" modifiers="NECESSARY:TRIGGERING">
            <acceptor>
                <param name="REGULAR" value="true"/>
            </acceptor>
       </input>
    </inputs>

    <outputs>
        <output name="outField" type="pl.edu.icm.visnow.lib.types.VNRegularField">
        </output>
    </outputs>
    <parameters>
    </parameters>
    <description value="interpolates input by bi/tri cubic splines to  given dimansions or by given density"/>
</module>
