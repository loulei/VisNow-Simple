<?xml version="1.0" encoding="UTF-8"?>

<module name="coordinate displacement" class="VectorFieldDisplacement">
    <inputs>
        <input name="inField" type="pl.edu.icm.visnow.lib.types.VNField" modifiers="NECESSARY:TRIGGERING">
        </input>
    </inputs>
    <outputs>
        <output name="outField" type="pl.edu.icm.visnow.lib.types.VNField">
        </output>
        <output name="regularOutField" type="pl.edu.icm.visnow.lib.types.VNRegularField">
        </output>
        <geometryOutput/>
    </outputs>
    <description value="displaces each node by the value of a vector component at this node, animates such displacement"/>
</module>
