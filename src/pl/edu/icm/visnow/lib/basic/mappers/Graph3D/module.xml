<?xml version="1.0" encoding="UTF-8"?>

<module name="graph 3D" class="Graph3D">
    <inputs>
        <input name="inField" type="pl.edu.icm.visnow.lib.types.VNRegularField" modifiers="NECESSARY:TRIGGERING">
            <acceptor>
                <param name="NDIMS" value="2"/>
            </acceptor>
        </input>
        <!--
        <input name="inTexture" type="pl.edu.icm.visnow.lib.types.VNImageObject" modifiers="TRIGGERING">
        </input>
        -->
    </inputs>
    <outputs>
        <output name="outField" type="pl.edu.icm.visnow.lib.types.VNRegularField">
            <schema>
                <param name="NSPACE" value="3"/>
                <param name="NDIMS" value="2"/>
            </schema>
        </output>
        <geometryOutput/>
    </outputs>    
    <parameters>
    </parameters>
    <description value="creates a grasph of a scalar component of a 2D regular field"/>
</module>
